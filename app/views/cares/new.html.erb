<%= form_with model: @user_category, url: cares_path, method: :post, class: 'registration-main',local: true do |f| %>
        <div class="contents">
          <div class='form-wrap'>
        <div class="form-group">

        <div class="form-group">
          <%#= render 'devise/shared/error_messages', model: f.object %>
          </div>
        <div class='form-text-wrap'>
          <label class="form-text">就職年月日</label>
          <span class="indispensable">必須</span>
        </div>
         <div class='input-birth-wrap'>
      <%= raw sprintf(
                  f.date_select(
                    :employment,
                    class:'select-employment',
                    id:"employment-date",
                    use_month_numbers: true,
                    prompt:'--',
                    start_year: 1950,
                    end_year: Time.now.year,
                    date_separator: '%s'),
                  "<p> 年 </p>", "<p> 月 </p>") + "<p> 日 </p>" %>
    </div>
      </div>
      <div class="form-group">
        <div class='form-text-wrap'>
          <label class="form-text">所属園</label>
          <span class="indispensable">必須</span>
        </div>
        <%= f.text_field :affiliation, class:"input-default", id:"addresses", placeholder:"たちばな幼稚園"%>
      </div>
      <div class="form-group">
        <div class='form-text-wrap'>
          <label class="form-text">リーダー</label>
          <span class="indispensable">必須</span>
        </div>
         <%= f.select :rank_id,  options_for_select(Rank.pluck(:name, :id)), {}, { class: "select-box", id: "prefecture" } %>
      </div>
      <div class="form-group">
        <div class='form-text-wrap'>
          <label class="form-text">履修</label>
          <span class="indispensable">必須</span>
        </div>
         <%= f.select :complete_id,  options_for_select(Complete.pluck(:name, :id)), {}, { class: "select-box", id: "prefecture" } %>
      </div>
      
      <div class='register-btn'>
    <%= f.submit "登録" ,class:"register-sub-btn" %>
    </div>
</div>
<% end %>