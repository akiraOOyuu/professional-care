<%= form_with model: @user_category, url: lecture_create_cares_path(@user_category), method: :patch, class: 'registration-main', local: true do |f| %>
        <div class="contents">
          <div class='form-wrap'>
        
        <div class="form-group">
        <div class="form-group">
           <%#= render 'devise/shared/error_messages', model: f.object %>
          </div>
       
        <div class='form-text-wrap'>
          <label class="form-text">研修分野</label>
          <span class="indispensable">必須</span>
        </div>
         <%= f.select :field_id,  options_for_select(Field.pluck(:name, :id)), {}, { class: "select-box", id: "prefecture" } %>
      </div>
      <div class="form-group">
      
        <div class='form-text-wrap'>
          <label class="form-text">講義名</label>
          <span class="indispensable">必須</span>
        </div>
        <%= f.text_field :lecture_name, class:"input-default", id:"addresses", placeholder:"幼児教育の重要性と現状"%>
      </div>
       <div class="form-group">
      
       <div class='form-text-wrap'>
          <label class="form-text">講義時間</label>
          <span class="indispensable">必須</span>
        </div>
        <%= f.text_field :lecture_time, class:"input-default", id:"addresses", placeholder:"90"%>
      </div>
      <div class="form-group">
      
        <div class='form-text-wrap'>
          <label class="form-text">講師名</label>
          <span class="indispensable">必須</span>
        </div>
        <%= f.text_field :instructor_name, class:"input-default", id:"addresses", placeholder:"斎藤　真紀子"%>
      </div>
      <div class="form-group">
      
        <div class='form-text-wrap'>
          <label class="form-text">受講日</label>
          <span class="indispensable">必須</span>
        </div>
         <div class='input-birth-wrap'>
      <%= raw sprintf(
                  f.date_select(
                    :lecture_day,
                    class:'select-employment',
                    id:"lecture_day-date",
                    use_month_numbers: true,
                    prompt:'--',
                    start_year: 1980,
                    end_year: Time.now.year,
                    date_separator: '%s'),
                  "<p> 年 </p>", "<p> 月 </p>") + "<p> 日 </p>" %>
    </div>
      </div>
      
      <div class='register-btn'>
    <%= f.submit "登録" ,class:"register-sub-btn" %>
    </div>
</div>
<% end %>